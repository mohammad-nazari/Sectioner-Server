   <!-- ns__User requestUser -->
   <requestUser>
    <!-- int uId -->
    <uId>0</uId>
    <!-- std::string uName -->
    <uName></uName>
    <!-- std::string uPassword -->
    <uPassword></uPassword>
    <!-- std::string uRePassword -->
    <uRePassword></uRePassword>
    <!-- std::string uFirstName -->
    <uFirstName></uFirstName>
    <!-- std::string uLastName -->
    <uLastName></uLastName>
    <!-- enum ns__UserType uType -->
    <uType>Monitor</uType>
    <!-- std::string uKey -->
    <uKey></uKey>
    <!-- ns__ErrorCode uErr -->
    <uErr>
     <!-- int eNo -->
     <eNo>0</eNo>
     <!-- std::string eMsg -->
     <eMsg></eMsg>
     <!-- enum ns__SettingLevel eType -->
     <eType>Normal</eType>
    </uErr>
   </requestUser>
  </ns:GetAllDeviceStatus>
 </SOAP-ENV:Body>
</SOAP-ENV:Envelope>
